<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="crexa_inc.summer2018">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">

        <activity android:name=".MainActivity">
            // main loads first
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RNGRGBActivity" />
        <activity android:name=".activity.BaseActivity" />
        <activity android:name=".activity.CustomDialogActivity" />
        <activity android:name=".activity.HistoryActivity" />
        <activity android:name=".activity.HarmonizorActivity" />
        <activity android:name=".activity.CustomColorActivity" />
        <activity android:name=".activity.MenuActivity" />
        <activity android:name=".activity.ForGradeActivity" />
        <activity android:name=".activity.PhotoColorActivity"></activity>
    </application>

</manifest>